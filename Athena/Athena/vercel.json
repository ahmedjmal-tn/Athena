{
  "build": [
      {
          "src": "Athena/wsgi.py",
          "use": "@vercel/python",
          "conf": {
              "maxLambdaSize": "15mn",
              "runtime": "python3.11.2"
          }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "Athena/wsgi.py"
      }
  ]
}
